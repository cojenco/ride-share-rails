<%= form_with model: @trip do |f| %>
  <p>Please provide the following information to edit your trip in our database:</p>

  <%= f.label :date %>
  <%= f.text_field :date %>
     <br>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
     <br>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
     <br>
  <%= f.submit "Edit" %>
<% end %>